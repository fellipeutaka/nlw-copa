generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider                  = "prisma-erd-generator"
  includeRelationFromFields = true
  DISABLE_ERD               = false
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pool {
  id        String   @id @default(uuid())
  title     String
  code      String   @unique
  createdAt DateTime @default(now())
}
